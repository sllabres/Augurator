<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />    
    <title>@ViewBag.Title</title>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyATWa8EJgsmG2HDQahhFQGYo8sk1DHVS2M&v=3.exp&sensor=false&libraries=places" type="text/javascript"></script>    
    <script src="~/Scripts/GoogleMaps.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-2.0.3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-1.10.3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.geocomplete.min.js" type="text/javascript"></script>
    <style type="text/css">
      html { height: 90% }
      body { height: 90%; margin: 0; padding: 0; }
      #rightPane { height: 100%; width: 85%; float: left; }
      #leftPane { height: 100%; width: 15%; float: left; }
      #sortableAddressList { list-style-type: none; margin: 0; padding: 0; }
      #sortableAddressList li { margin: 3px 3px 3px 0; padding: 1px; float: left; width: 200px; height: 50px; text-align: center; border: 1px solid black; }  

      #sortableDestinations { list-style-type: none; margin: 0; padding: 0; }
      #sortableDestinations li { margin: 3px 3px 3px 0; padding: 1px; float: left; width: 100px; height: 90px; text-align: center; border: 1px solid black; }  
    </style>
</head>
<body>    
        @RenderBody()    
</body>
</html>
